name: dev

on:
  push:
    branches: [ "main" ]
  pull_request:
    branches: [ "main" ]

  workflow_dispatch:

  build:
    runs-on: ubuntu-latest
    environment: dev    
    steps:      
      - uses: .github/actions/build

  deploy: 
    runs-on: ubuntu-latest
    needs: [build]
    environment: dev
    steps:
      - name: print url var 
        run: echo ${{ vars.url }}
        

        
  
